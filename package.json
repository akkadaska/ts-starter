{
  "name": "ts-starter",
  "version": "1.0.0",
  "description": "My TypeScript starter template",
  "private": true,
  "scripts": {
    "start": "ncc run src/index.ts -s",
    "build": "ncc build src/index.ts -m --license LICENSE",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts' 'test/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "ci:test:coverage": "jest --coverage --silent --ci --testLocationInResults --json --outputFile=\"report.json\""
  },
  "keywords": [],
  "author": "Daisuke Akazawa",
  "devDependencies": {
    "@swc/core": "^1.3.24",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  }
}
